{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "build server",
      "command": "/usr/bin/gcc",
      "args": [
        "-g",
        "${file}",
        "-pthread",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}"
      ],
      "options": {
        "cwd": "/usr/bin"
      },
      "problemMatcher": ["$gcc"],
      "group": "build"
    }
  ]
}
